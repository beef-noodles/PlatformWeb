{
  "name": "summit-web",
  "version": "0.1.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "vendorDev": "cross-env NODE_ENV=development webpack --config webpack.vendor.js",
    "vendorPro": "cross-env NODE_ENV=production webpack --config webpack.vendor.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "live": "live-server ./build",
    "lint": "tslint ./src/**/*.tsx",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "doc": "styleguidist build",
    "docServer": "styleguidist server",
    "getstorybook": "getstorybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "buildImage": "docker build -t summit . && docker tag summit guzhongren/summit:latest",
    "pushImage": "docker push guzhongren/summit:latest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tslint ./src/**/*.tsx",
      "pre-push": "tslint ./src/**/*.tsx"
    }
  },
  "standard": {
    "globals": [
      "console",
      "Error",
      "config",
      "$",
      "antd",
      "woff"
    ],
    "properties": false,
    "parser": "babel-eslint",
    "ignore": [
      "./public/dist/",
      "./build/",
      "./styleguide/"
    ]
  },
  "browsers": [
    ">5%",
    "last 4 versions",
    "Firefox ESR",
    "Edge",
    "ie >= 9"
  ],
  "dependencies": {
    "@material-ui/core": "^3.0.2",
    "@material-ui/icons": "^3.0.1",
    "@types/react-router-dom": "^4.2.7",
    "antd": "^3.8.2",
    "axios": "^0.18.0",
    "echarts": "^4.2.0-rc.1",
    "echarts-for-react": "^2.0.15-beta.0",
    "font-awesome": "^4.7.0",
    "less": "^3.8.1",
    "maptalks": "^0.41.1",
    "pubsub-js": "^1.6.0",
    "re-animate": "^0.0.3",
    "react": "^16.4.0",
    "react-csv-reader": "^0.3.2",
    "react-dom": "^16.4.0",
    "react-draggable": "^3.0.5",
    "react-icons": "^3.0.1",
    "react-router-dom": "^4.3.1",
    "velocity-animate": "^1.5.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.8",
    "@storybook/addon-links": "^3.4.8",
    "@storybook/addons": "^3.4.8",
    "@storybook/cli": "^3.4.8",
    "@storybook/react": "^3.4.8",
    "@types/jest": "^23.0.0",
    "@types/node": "^10.3.2",
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "awesome-typescript-loader": "^5.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-runtime": "^6.26.0",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^0.1.19",
    "console-polyfill": "^0.3.0",
    "conventional-changelog-cli": "^2.0.5",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-hot-loader": "^1.3.9",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.1",
    "del-cli": "^1.1.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "file-loader": "^2.0.0",
    "happypack": "^5.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.9",
    "less-loader": "^4.1.0",
    "live-server": "^1.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "postcss-calc": "^6.0.1",
    "postcss-color-rgba-fallback": "^3.0.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-flexibility": "^2.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-opacity": "^5.0.0",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react-docgen-typescript": "^1.6.1",
    "react-styleguidist": "^7.0.20",
    "resolve-url-loader": "^2.3.0",
    "source-map-loader": "^0.2.3",
    "standard": "^12.0.1",
    "style-loader": "^0.23.0",
    "ts-import-plugin": "^1.5.4",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.11.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-parallel-uglify-plugin": "^1.1.0"
  }
}
